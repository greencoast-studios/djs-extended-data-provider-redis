{
  "name": "@greencoast/djs-extended-data-provider-redis",
  "version": "1.0.0",
  "description": "[discord.js-extended] - A data provider implemented with Redis.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "jest --silent",
    "test:showlogs": "jest",
    "test:watch": "jest --watchAll --silent",
    "test:watch-showlogs": "jest --watchAll",
    "build": "tsc --build",
    "prepublish": "npm run build",
    "docs": "typedoc ./src/index.ts --includeVersion --out ./docs --readme ./README.md --plugin typedoc-plugin-extras --favicon ./assets/favicon.ico",
    "docs:watch": "npm run docs -- --watch"
  },
  "engines": {
    "node": ">=16.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greencoast-studios/djs-extended-data-provider-redis.git"
  },
  "keywords": [
    "greencoast",
    "greencoast-studios",
    "discord",
    "discord.js",
    "discord bots",
    "discord.js-extended",
    "discord.js-extended data provider",
    "redis"
  ],
  "author": "Greencoast Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greencoast-studios/djs-extended-data-provider-redis/issues"
  },
  "homepage": "https://github.com/greencoast-studios/djs-extended-data-provider-redis#readme",
  "devDependencies": {
    "@greencoast/discord.js-extended": "^3.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "discord.js": "^14.11.0",
    "eslint": "^8.45.0",
    "eslint-config-greencoast": "^0.0.4",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@greencoast/discord.js-extended": ">=3.0.1",
    "discord.js": "~14"
  },
  "dependencies": {
    "redis": "^4.6.7"
  }
}
