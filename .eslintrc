{
  "extends": [
    "greencoast/node",
    "greencoast/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "no-dupe-class-members": "off"
  },
  "overrides": [
    {
      "files": ["__mocks__/**/*", "test/**/*"],
      "rules": {
        "camelcase": "off",
        "dot-notation": "off",
        "prefer-destructuring": "off",
        "no-global-assign": "off",
        "max-lines": "off",
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}